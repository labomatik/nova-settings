{
    "name": "rdvbiz/nova-settings",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.34"
    },
    "autoload": {
        "psr-4": {
            "Rdvbiz\\NovaSettings\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rdvbiz\\NovaSettings\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
